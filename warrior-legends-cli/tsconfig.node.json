{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [
    "electron.vite.config.*",
    "src/main/**/*",
    "src/preload/**/*"
  ],
  "compilerOptions": {
    "composite": true,
    "types": [
      "electron-vite/node"
    ],
    //不检查未使用的变量
    "noUnusedLocals": false,
    //不检查未使用的参数
    "noUnusedParameters": false
  }
}
